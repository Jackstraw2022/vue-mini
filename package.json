    "@vitest/coverage-istanbul": "^1.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.3.3",
    "vitepress": "1.0.0-rc.33",
    "vitest": "^1.1.1",
    "xo": "^0.56.0"
